<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Base application theme. -->
    <style name="Base.Theme.LAMAJ" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
        <item name="android:statusBarColor">@color/background_color_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowBackground">@color/background_color_dark</item>
        <item name="android:navigationBarColor">@color/background_color_dark</item>
    </style>

    <style name="background">
        <item name="android:background">@color/background_color_dark</item>
    </style>

    <style name="MainTitle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">30dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:fontFamily">@font/lovelo_line</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="Title">
        <item name="android:textSize">30dp</item>
        <item name="android:textColor">@color/textButtonColor_dark</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Text">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/TextColor_dark</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Import">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/textButtonColor_dark</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:background">@drawable/button_background_dark</item>
        <item name="backgroundTint">@color/buttonColor_dark</item>
        <item name="android:textStyle">bold</item>
        <item name="android:drawableLeft">@drawable/import_data</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="drawableTint">@color/textButtonColor_dark</item>
    </style>

    <style name="Save">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/textButtonColor_dark</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:background">@drawable/button_background_dark</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:paddingRight">40dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="backgroundTint">@color/buttonColor_dark</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Cancel">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/textButtonColor_dark</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:background">@drawable/button_background_dark</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="backgroundTint">@color/transparent</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="SettingsButton">
        <item name="android:background">@drawable/setting</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="SettingsBox">
        <item name="android:background">@drawable/box_up_dark</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">10dp</item>
        <item name="tint">@color/background_color_dark</item>
        <item name="android:textColorHint">@color/TextColor_dark</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="SettingsName">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/TextColor_dark</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">15dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="SettingsSwitch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="BoxColor">
        <item name="android:background">@drawable/pop_up_dark</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Empty">
        <item name="android:background">@drawable/empty_dark</item>
        <item name="android:layout_below">@id/body</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="EmptyImg">
        <item name="tint">@color/TextColor_dark</item>
        <item name="android:src">@drawable/box</item>
    </style>

    <style name="sharkImg">
        <item name="tint">@color/buttonColor_dark</item>
        <item name="android:src">@drawable/requin</item>
    </style>

    <style name="SearchBar">
        <item name="android:background">@drawable/button_background_dark</item>
        <item name="android:backgroundTint">@color/box_color_dark</item>
        <item name="android:textColorHint">@color/TextColor_dark</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="SearchButton">
        <item name="android:background">@drawable/button_background_dark</item>
        <item name="android:tint">@color/textButtonColor_dark</item>
    </style>

    <style name="SearchSettings">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/textButtonColor_dark</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:background">@drawable/button_background_dark</item>
        <item name="android:backgroundTint">@color/backgroundtint_selector_dark</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:button">@drawable/drawable_rotation_selector</item>
        <item name="buttonTint">@color/drawabletint_selector_dark</item>
    </style>

    <style name="FrameContainer">
        <item name="android:background">@drawable/button_background_dark</item>
        <item name="android:backgroundTint">@color/box_color_dark</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="FrameText">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/TextColor_dark</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="SelectFrame">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@drawable/selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/transparent</item>
        <item name="android:tint">@color/textButtonColor_dark</item>
        <item name="android:padding">0dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

</resources>